<div class="fixed-aside">
	Add an optional catcher for your charms
</div>


<a href="#" class="btn next f-right">next</a>

<div class="page-content">

	<div class="holders f-left">
		<% var h;
		for(var i=0; i<holders.length; i++) { h = holders[i]; %>
			<div class="product">
				<a href="#" data-product-sku="<%= h.sku %>">
					<img src="<%= h.images.thumb %>" alt="" class="holder-img" />
					<img src="<%= h.images.thumb_detail %>" alt="" class="rollover" />
					<h4 class="name"><%= h.name %></h4>
					<h5 class="price">$<%= h.price %></h5>
				</a>
			</div>
		<% } %>
	</div>

	<div class="form f-left">
		<img src="<%= form.images.full %>" alt="form.name" />
		<div class="form-holder"></div>
	</div>
	<div class="clearfix"></div>
</div>

<a href="#" class="btn next f-right">next</a>
<div class="clearfix"></div>
